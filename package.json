{
  "name": "marvel-spa",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "node --watch server/proxy.js",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node server/proxy.js"
  },
  "dependencies": {
    "@comic-vine/client": "^0.2.0",
    "@vueuse/core": "^14.2.0",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "pinia": "^3.0.4",
    "vite-plugin-node-polyfills": "^0.25.0",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "packageManager": "pnpm@10.28.2"
}
